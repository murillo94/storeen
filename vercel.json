{
  "version": 2,
  "name": "storeen",
  "alias": "storeen",
  "builds": [
    { "src": "packages/web/package.json" },
    { "src": "packages/styleguide/package.json" }
  ],
  "routes": [
    {
      "src": "/styleguide",
      "dest": "/packages/styleguide/index"
    },
    {
      "src": "/styleguide/(.*)",
      "dest": "/packages/styleguide/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/web/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
