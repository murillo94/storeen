{
  "version": 2,
  "name": "storeen",
  "alias": "storeen",
  "builds": [
    { "src": "packages/web/package.json", "use": "@now/next" },
    { "src": "packages/styleguide/package.json", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "/styleguide",
      "dest": "/packages/styleguide/index",
      "continue": true
    },
    {
      "src": "/styleguide/(.*)",
      "dest": "/packages/styleguide/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/web/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
