{
  "version": 2,
  "name": "storeen",
  "alias": "storeen",
  "builds": [{ "src": "packages/web/next.config.js", "use": "@now/next" }],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+",
      "dest": "/packages/web/_next/static/$1",
      "headers": { "cache-control": "immutable,max-age=31536000" }
    },
    {
      "src": "/(.*)",
      "dest": "/packages/web/$1"
    }
  ]
}
