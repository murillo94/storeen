{
  "version": 2,
  "name": "storeen",
  "alias": "storeen",
  "builds": [
    { "src": "packages/web/package.json", "use": "@now/next" },
    { "src": "packages/styleguide/package.json", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/packages/web/index"
    },
    {
      "src": "/styleguide/",
      "dest": "/packages/styleguide/index"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/web/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
